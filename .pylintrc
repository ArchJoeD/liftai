[MASTER]

[MESSAGES CONTROL]

disable=all

enable=F,
       E,
       unreachable,
       duplicate-key,
       unnecessary-semicolon,
       global-variable-not-assigned,
       unused-import,
       unused-variable,
       binary-op-exception,
       bad-format-string,
       anomalous-backslash-in-string,
       bad-open-mode,
       reimported,
       multiple-imports,
       wrong-import-order

ignored-modules=sqlalchemy
